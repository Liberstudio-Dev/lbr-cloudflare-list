    "@nestjs/core": "^11.1.14",
    "@types/express": "^5.0.6",
    "@types/node": "^20.9.0",
    "axios": "^1.0.0",
    "rxjs": "^7.8.0",
    "simple-git-hooks": "^2.13.1",
    "typescript": "^5.0.0"
